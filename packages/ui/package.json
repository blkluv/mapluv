{
    "name": "ui",
    "version": "0.7.3",
    "description": "3D Rendering Engine for GIS",
    "keywords": [
      "3D",
      "sdk",
      "api",
      "webgl",
      "map",
      "geospatial",
      "globe"
    ],
    "main": "dist/index.js",
    "directories": {
      "doc": "doc"
    },
    "scripts": {
      "release": "cross-env WEBPACK_ENV=dist-ui webpack -p",
      "debug": "cross-env WEBPACK_ENV=ui webpack --watch -d",
      "css": "postcss -o dist/mapray.css src/css/mapray.css",
      "css-devel": "postcss --watch -o dist/mapray.css src/css/mapray.css"
    },
    "contributors": [
      {
        "name": "Yasuhisa Tanioka"
      },
      {
        "name": "Daisuke Matsumoto",
        "url": "https://github.com/daisuke201"
      },
      {
        "name": "masa-sony"
      }
    ],
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/sony/mapray-js.git"
    },
    "bugs": {
      "url": "https://github.com/sony/mapray-js/issues"
    },
    "homepage": "https://github.com/sony/mapray-js",
    "devDependencies": {
      "babel-core": "^6.21.0",
      "babel-loader": "^6.2.10",
      "babel-plugin-add-module-exports": "^0.2.1",
      "babel-preset-env": "^1.7.0",
      "cross-env": "^5.1.0",
      "cssnano": "^4.1.10",
      "jsdoc": "^3.5.0",
      "postcss-cli": "^6.1.3",
      "postcss-inline-svg": "^4.1.0",
      "webpack": "^1.14.0"
    },
    "dependencies": {}
  }