  },
  "homepage": "https://github.com/sony/mapray-js",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.0",
