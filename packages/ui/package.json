{
  "name": "@mapray/ui",
  "version": "0.7.3",
  "description": "3D Rendering Engine for GIS",
  "keywords": [
    "3D",
    "sdk",
    "api",
    "webgl",
    "map",
    "geospatial",
    "globe"
  ],
  "main": "dist/mapray-ui.js",
  "directories": {
    "doc": "doc"
  },
