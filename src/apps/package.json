      }
    ],
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/sony/mapray-js.git"
    },
    "bugs": {
      "url": "https://github.com/sony/mapray-js/issues"
    },
    "homepage": "https://github.com/sony/mapray-js",
    "devDependencies": {
      "babel-core": "^6.21.0",
      "babel-loader": "^6.2.10",
      "babel-plugin-add-module-exports": "^0.2.1",
      "babel-preset-env": "^1.7.0",
      "cross-env": "^5.1.0",
