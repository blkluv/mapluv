{
    "name": "mapray-js",
    "version": "0.7.3",
    "description": "3D Rendering Engine for GIS",
    "keywords": [
      "3D",
      "sdk",
      "api",
      "webgl",
      "map",
      "geospatial",
      "globe"
    ],
    "main": "dist/mapray.js",
    "directories": {
      "doc": "doc"
    },
    "scripts": {
      "apps": "cross-env WEBPACK_ENV=apps webpack -p",
      "apps-devel": "cross-env WEBPACK_ENV=apps webpack --watch -d",
      "start-server": "st --no-cache -H 0.0.0.0 --port 7776 --index index.html ."
    },
    "contributors": [
